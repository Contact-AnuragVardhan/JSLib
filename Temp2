221: NSUtil.prototype.isElementOfType = function (element,tagName)
{
	if(element && tagName)
    {
        if(element.tagName && element.tagName.toLowerCase() === tagName.toLowerCase())
        {
        	return true;
        }
    }
	return false;
};
